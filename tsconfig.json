{
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ],
    "strict": true,
    "rootDirs": ["src"],
    "outDir": "./bin",
    "target": "es5",
    "skipLibCheck": true,
    "declaration": true,
    "types": [
      "node",
      "@slack/types",
      "@types/jest",
      "jest",
      "commander/typings",
      "@types/dotenv",
      "slack-mock-typed",
      "aws-sdk-mock",
    "bluebird"]
  }
  ,
  "exclude": [
    "./node_modules/cypress/types",
    "./node_modules/cypress-testing-library/typings",
    "./untyped.d.ts"
  ]
  ,
  "include": [
    "./src",
  ]
,
}
