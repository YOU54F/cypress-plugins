{
  "name": "cypress-slack-reporter",
  "version": "0.2.8",
  "description": "A slack reporter for mochawesome reports generated by Cypress or other test frameworks using Mocha, for runs generated on CircleCI",
  "main": "./bin/index.js",
  "bin": {
    "cypress-slack-reporter": "./bin/index.js",
    "cypress-slack-reporter-full": "./bin/cli/spec.js"
  },
  "keywords": [
    "cypress",
    "slack",
    "circleci",
    "reporter",
    "mocha",
    "mochawesome",
    "mochawesome-merge",
    "marge"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/you54f/cypress-slack-reporter.git"
  },
  "author": "Yousaf Nabi",
  "license": "MIT",
  "scripts": {
    "cli": "rimraf cypress/reports && ts-node src/cli/spec.ts",
    "circle": "npm run lint && npm run test:ci && make test && npm run pre-publish",
    "lint": "tslint --project .",
    "lint-fix": "tslint --project . --fix",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "compile": "tsc",
    "slack-js": "node bin/index.js --logger",
    "slack-js-opts-build-failed": "node bin/index.js --report-dir src/slack/test/jsonBuildFail --video-dir src/slack/test/videosDirPopulated --screenshot-dir src/slack/test/screenshotDirPopulated --logger",
    "slack-js-opts-test-failed": "node bin/index.js --report-dir src/slack/test/jsonTestFail --video-dir src/slack/test/videosDirPopulated --screenshot-dir src/slack/test/screenshotDirPopulated --logger",
    "slack-js-opts-test-passed": "node bin/index.js --report-dir src/slack/test/jsonTestPass --video-dir src/slack/test/videosDirPopulated --screenshot-dir src/slack/test/screenshotDirPopulated --logger",
    "slack-js-opts": "node bin/index.js --report-dir src/slack/test/reportSingle --video-dir src/slack/test/videosDirPopulated --screenshot-dir src/slack/test/screenshotDirPopulated --logger",
    "slack-ts": "ts-node src/index.ts --logger",
    "slack-ts-opts": "ts-node src/index.ts --report-dir src/slack/test/reportSingle --video-dir src/slack/test/videosDirPopulated --screenshot-dir src/slack/test/screenshotsDirPopulated --logger",
    "ci": "npm run test:ci && rimraf ./bin && npm run compile",
    "ship-it": "npm run pre-publish && npm publish",
    "deploy:prepare": "./scripts/create_npmrc_file.sh",
    "release": "standard-version"
  },
  "dependencies": {
    "@slack/types": "^1.0.0",
    "@slack/webhook": "^5.0.0",
    "commander": "^2.20.0"
  },
  "peerDependencies": {
    "mochawesome": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@cypress/webpack-preprocessor": "4.0.4",
    "@types/bluebird": "^3.5.27",
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.12",
    "@types/node": "10.14.4",
    "aws-sdk": "^2.452.0",
    "aws-sdk-mock": "^4.4.0",
    "coveralls": "3.0.3",
    "cypress": "3.3.1",
    "cypress-cucumber-preprocessor": "1.11.2",
    "dotenv": "8.0.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "jest-stare": "1.14.0",
    "lint-staged": "8.1.7",
    "mocha": "6.1.4",
    "mocha-junit-reporters": "1.23.6",
    "cypress-multi-reporters": "1.1.20",
    "mochawesome": "3.1.2",
    "mochawesome-merge": "1.0.7",
    "mochawesome-report-generator": "3.1.5",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "slack-mock-typed": "2.1.2",
    "standard-version": "6.0.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "typescript": "3.5.1",
    "webpack": "4.32.2",
    "winston": "2.4.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest-stare": {
    "coverageLink": "../lcov-report/index.html",
    "resultDir": "coverage/html"
  },
  "jest-junit": {
    "outputDirectory": "./testReports/",
    "reportDir": "testReports/unit"
  }
}
